Unable to find some of the cards. Please check possible matches below.

<ul>
<% for error_card in @error_cards %>
<li><%= error_card[:name] %></li>
<% end %>
</ul>

<%= form_with class: "form", url: "/decks/#{@master_deck.slug}/branch/#{@branch.slug}/fixedcards" do |f| %>
<div class="row">
    <div class="col">
        <div class="card">
            <div class="card-header  card-header-text card-header-primary"><div class="card-text">
              <h4 class="card-title">Check the correct cards to add</h4></div>
             </div>
            <div class="card-body">
                
                <% unless @possible_fixed_cards.empty? %>
                    <ul class="error-card-list">
                        <% for card in @possible_fixed_cards %>
                        <li><div class="form-check">
                            <label class="form-check-label">
                                <input class="form-check-input" type="checkbox" name="fixed_cards[<%= @possible_fixed_cards.index(card) %>]" id="fixed_cards[<%= @possible_fixed_cards.index(card) %>]">
                                <%= card.oracle_name %> <img src="<%= card.image(size: 'small') %>" /> 
                                <span class="form-check-sign">
                                    <span class="check"></span>
                                </span>
                            </label>
                        </div>
                </li>
                        <% end %>
                        </ul>
                
                <% end %>
                
            </div>
            <div class="card-footer">
                <button type="submit" class="btn btn-primary">Submit Fixed Cards</button>
                
            </div>
        </div>
        
        
    </div>
    
</div>
<% end %>